{% extends 'base.html.twig' %}

{% block title %}Contact{% endblock %}


{% block body %}
    <div class="row text-center pt-5">
        <div class="col-12">
            <h1 class="">Contact form</h1>            
        </div>
    </div>   
     <div class="row py-5">        
        <div class="col-12">
            {% for message in app.flashes('success') %}
            <div class="alert alert-success">
                {{ message }}
            </div>
            {% endfor %}
            {{ form(form) }}
        </div>
    </div>  
{% endblock %}
